{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "packages/frontend/dist",
  "functions": {
    "api/database-backend.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/database-backend.js"
    }
  ]
} 